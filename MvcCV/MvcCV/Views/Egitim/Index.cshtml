@using MvcCV.Models.Entity
@model List<Tbl_Education>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h2>Eğitim Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Alt Başlık</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ID</td>
            <td>@x.Title</td>
            <td>@x.SecondTitle</td>
            <td>@x.SecondTitle2</td>
            <td>@x.NoteSum</td>
            <td>@x.Date</td>
            <td><a href="/Egitim/EgitimSil/@x.ID" class="btn btn-outline-danger">Sil</a></td>
            <td><a href="/Egitim/EgitimDuzenle/@x.ID" class="btn btn-outline-warning">Düzenle</a></td>


        </tr>
    }
</table>
<a href="/Egitim/EgitimEkle/" class="btn btn-outline-primary">Yeni Eğitim Bilgisi Ekle</a>

