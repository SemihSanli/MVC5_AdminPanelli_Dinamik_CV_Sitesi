@using MvcCV.Models.Entity
@model List<Tbl_Experience>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h2>Deneyim Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Alt Başlık</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
    </tr>
    @foreach(var x in Model)
    {
    <tr>
        <td>@x.ID</td>
        <td>@x.Title</td>
        <td>@x.SecondTitle</td>
        <td>@x.Date</td>
        <td><a href="/Deneyim/deneyimSil/@x.ID" class="btn btn-outline-danger">Sil</a></td>
        <td><a href="/Deneyim/DeneyimGetir/@x.ID" class="btn btn-outline-warning">Düzenle</a></td>
        
        
    </tr>
    }
</table>
<a href="/Deneyim/DeneyimEkle/" class="btn btn-outline-primary">Yeni Deneyim Ekle</a>

