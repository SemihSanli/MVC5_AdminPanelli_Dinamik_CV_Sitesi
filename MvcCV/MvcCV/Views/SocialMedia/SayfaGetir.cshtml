@model MvcCV.Models.Entity.Tbl_SocialMedia
@{
    ViewBag.Title = "SayfaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<br />
<h4>Sosyal Medya Hesap Düzenleme</h4>
<br />
<form method="post">



    @Html.Label("Sosyal Medya Adı ")
    @Html.TextBoxFor(x => x.nameS, new { @class = "form-control" })
    <br />
    @Html.Label("Sosyal Medya Linki")
    @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Sosyal Medya İkonu")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
    <br />

    <button class="btn btn-outline-info">Kaydet</button>
</form>
