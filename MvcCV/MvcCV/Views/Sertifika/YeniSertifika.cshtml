@model MvcCV.Models.Entity.Tbl_Certificate
@{
    ViewBag.Title = "YeniSertifika";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}






<br />
<h4>Yeni Sertifika Ekleme Sayfası</h4>
<br />
<form method="post">



    @Html.Label("Sertifika Açıklaması")
    @Html.TextBoxFor(x => x.Description, new { @class = "form-control" })
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Date, new { @class = "form-control" })
    <br />

    <button class="btn btn-outline-info">Kaydet</button>
</form>

