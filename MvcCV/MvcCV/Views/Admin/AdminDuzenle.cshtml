@model MvcCV.Models.Entity.Tbl_Admin
@{
    ViewBag.Title = "AdminDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<br />
<h4>Kullanıcı Güncelleme</h4>
<br />
<form method="post">

    @Html.Label("Kullanıcı ID")
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
    <br />
    @Html.Label("Kullanıcı Adı")
    @Html.TextBoxFor(x => x.Username, new { @class = "form-control" })
    <br />
    @Html.Label("Şifre")
    @Html.TextBoxFor(x => x.Password, new { @class = "form-control" })
    <br />


    <button class="btn btn-outline-info">Güncelle</button>
</form>



